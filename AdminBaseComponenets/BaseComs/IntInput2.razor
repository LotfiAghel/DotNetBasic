@typeparam TItem 



@using Microsoft.AspNetCore.Components.Rendering;




@using System.Collections.Generic


@using Newtonsoft.Json.Linq;


@inherits ValueInput<TItem>


        <NumericEdit    
             ReadOnly=@ReadOnly
             TValue=TItem
             Value=value
             ValueChanged=@Click22 
        />






@code
{
    
    TItem prvValue;
    

    async Task Click22(TItem vv)
    {
        value = vv;
        if (!prvValue.Equals(value))
        {

            OnChange(value);
        }
        prvValue = value;

    }

}

