@typeparam TEntity
@typeparam TKEY




@using Microsoft.AspNetCore.Components.Rendering;



@using Models
@using System.Collections.Generic


@using Newtonsoft.Json.Linq;

@inherits IntegerForeignKeyInput<TEntity,TKEY>

@{
    Data =default(TEntity);
    var tmp = Program0.getEntityManager<TEntity, TKEY>();

     Data = tmp.getFromLoaded(value.Value);
     if(Data!=null){
        viewComponenet = Program0.createWidget(Data.GetType(), new List<Attribute>() {  });
        dataId = Data.id;
     }

    
}


<div class="row pt-2 pb-2" @key=@dataId>
    @if(Data!=null && viewComponenet != null){
        <div class="col-sm-10">
            <div  @key=@dataId>@Program0.CreateDynamicComponent2(this, viewComponenet, Data)</div>
        </div>
    }else{
        <div>@value.Value</div>
    }
    
</div>





